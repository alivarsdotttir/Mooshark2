@model Mooshark2.Models.Entities.Subproject


@using (Html.BeginForm("Submit", "Student", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <!--Html.EditorFor(m => m)-->
    
    @Html.HiddenFor(x => x.ID)
    @Html.HiddenFor(x => x.Name)
    @Html.HiddenFor(x => x.Description)
    @Html.HiddenFor(x => x.ProjectID)
    @Html.HiddenFor(x => x.Weight)
    @Html.HiddenFor(x => x.Input)
    @Html.HiddenFor(x => x.Output)

    <div class="jumbotron">
        <div class="row">
            <div class="col-sm-8">
                <h2>Submit your solution to @Model.Name</h2>
                <input type="file" name="file" class="btn btn-lg" /><br />
            </div>
            <div class="col-sm-4">
                <input type="submit" name="Submit" id="Submit" value="Upload" class="btn btn-lg AdminButtons" />
            </div>
        </div>
    </div>

    
}

