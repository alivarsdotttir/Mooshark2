@model IEnumerable<Mooshark2.Models.DAL.ApplicationUser>

@{
    ViewBag.Title = "UserEditList";
}

<h2>All Users</h2>

    <table class="table table-bordered table-striped">
        <caption></caption>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>SSN</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var user in Model)
            {
                <tr>
                    <td>
                        <a href="/Admin/EditUser/@user.Id"><img alt="Edit" src="@Url.Content("~/Images/edit.png")" width="20" height="20"/></a>
                        @user.FullName
                    </td>
                    <td>
                        @user.Email
                    </td>
                    <td>
                        @user.SSN
                    </td>
                </tr>
            }

        </tbody>
    </table>

